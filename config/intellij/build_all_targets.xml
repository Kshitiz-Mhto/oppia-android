<configuration name="Build All Targets" factoryName="Bazel Command"
  folderName="Builds" type="BlazeCommandRunConfigurationType">
  <blaze-settings blaze-command="build"
    handler-id="BlazeCommandGenericRunConfigurationHandlerProvider">
    <blaze-target>//...</blaze-target>
    <blaze-user-flag>--keep_going</blaze-user-flag>
  </blaze-settings>
  <method v="2">
    <option name="Blaze.BeforeRunTask" enabled="true" />
  </method>
</configuration>

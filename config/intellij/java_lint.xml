<configuration name="Java Lint" factoryName="Bazel Command" folderName="Static Checks"
  type="BlazeCommandRunConfigurationType">
  <blaze-settings blaze-command="run"
    handler-id="BlazeCommandGenericRunConfigurationHandlerProvider">
    <blaze-target>//scripts:checkstyle</blaze-target>
    <blaze-user-exe-flag>$PROJECT_DIR$/..</blaze-user-exe-flag>
  </blaze-settings>
  <method v="2">
    <option name="Blaze.BeforeRunTask" enabled="true" />
  </method>
</configuration>
